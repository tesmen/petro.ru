{% extends '_base.html.twig' %}
{% block css %}
    <link rel="stylesheet" type="text/css" href="css/slides.css"/>
{% endblock %}
{% block javascript %}
    <script src="js/slides.js"></script>
    <script>
        $(function () {
            $("#slides").slides({
                responsive: true,
                height: 200
            });
        });
        $("document").ready(function () {
            $("#slides").slides("play");
        });
    </script>
{% endblock %}

{% block title %}  {{ title }}  {% endblock %}

{% block content %}
about
{% endblock %}